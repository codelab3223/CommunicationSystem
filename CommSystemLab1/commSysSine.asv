A=1;
f=0.5;
t=-5:0.01:5;
y=A * sin(2*pi*f*t);
subplot(2,1,2)
plot(t,y);
xlabel('time domain(n)')
ylabel ('amplitude del[n]')
title('continuous time sinusoidal signal ');
%axis([-3 3 -1 2])
grid on

